int a 0
int b 0
int c 1

clear
print "Guess the number from 1 to 1000!" ` "1) Your guess: "
rand 1 1000 b
scan a

doif $a > $b goto 18
doif $a < $b goto 21

print "You win!" ` "Press any key to restart"
swap c 1
scan
goto 5

print "Less! " `
goto 23

print "More! " `

sum c 1
print $c ") Try again: "
goto 8
